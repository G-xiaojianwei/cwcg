@import "../../common/vendor.wxss";

.info {
  display: flex;
  align-items: center;
  justify-content: center;
  padding: 20rpx 40rpx;
  background: #fff;
  border-bottom: 20rpx solid #F1F2F6;
  border-top: 20rpx solid #F1F2F6;
}
.info ._div:nth-child(1) {
  width: 30%;
  padding-right: 20rpx;
}
.info ._div:nth-child(1) ._img {
  width: 100%;
  height: 170rpx;
  vertical-align: top;
}
.info ._div:nth-child(2) {
  width: 70%;
}
.info ._div:nth-child(2) ._p:nth-child(1) {
  font-weight: bold;
  font-size: 28rpx;
}
.info ._div:nth-child(2) ._p:nth-child(2) {
  font-size: 26rpx;
  padding: 10rpx 0;
  color: #666;
}
.info ._div:nth-child(2) ._p:nth-child(3) {
  font-size: 24rpx;
  color: #F43530;
}
.info ._div:nth-child(2) ._p:nth-child(3) ._span:nth-child(2) {
  float: right;
  color: #000;
}
.type {
  padding: 20rpx 40rpx;
  border-bottom: 20rpx solid #F1F2F6;
}
.type ._span {
  font-weight: bold;
  font-size: 30rpx;
}
.icon-sl {
  float: right;
  display: block;
  width: 40rpx;
  height: 40rpx;
  background: url(data:image/png;base64,);
  background-size: 100% 100%;
}
.reason {
  padding: 20rpx 40rpx;
}
.reason .title {
  font-weight: bold;
  font-size: 30rpx;
}
.reason .title ._span {
  font-weight: 100;
  font-size: 24rpx;
  color: #666;
  line-height: 30rpx;
  margin-left: 20rpx;
}
.cont ._textarea {
  margin-top: 40rpx;
  padding: 20rpx;
  box-sizing: border-box;
  font-size: 26rpx;
  width: 100%;
  height: 300rpx;
  border: 1rpx solid #F1F2F6;
}
.file-img {
  display: inline-block;
  vertical-align: middle;
  margin-right: 18rpx;
  position: relative;
  margin-top: 20rpx;
  width: 150rpx;
  height: 200rpx;
  border: 1rpx solid #F1F2F6;
}
.file-img:last-child {
  margin-right: 0;
}
.file-img ._img {
  position: absolute;
  left: 0;
  right: 0;
  top: 0;
  bottom: 0;
  margin: auto;
  width: 50%;
  height: 40%;
}
.btn {
  position: fixed;
  left: 0;
  bottom: 0;
  width: 100%;
  height: 80rpx;
  background: #F43530;
  color: #fff;
  font-size: 30rpx;
  border-radius: 0 !important;
}
.btn2 {
  position: fixed;
  left: 0;
  bottom: 0;
  width: 100%;
  height: 80rpx;
  background: #F43530;
  color: #fff;
  font-size: 30rpx;
  border-radius: 0 !important;
}
.type-model {
  z-index: 10;
  width: 100%;
  height: 100%;
  position: fixed;
  left: 0;
  top: 0;
  background: rgba(0,0,0,.7);
}
.type-model .model-cont {
  width: 100%;
  height: 90%;
  position: absolute;
  left: 0;
  bottom: 0;
  background: #fff;
  padding: 40rpx;
  box-sizing: border-box;
}
.model-title {
  text-align: center;
  padding: 30rpx 0;
  font-weight: bold;
}
.model-tag {
  margin-top: 20rpx;
  display: flex;
  align-items: center;
  justify-content: center;
  border-bottom: 1rpx solid #FE223B;
}
.model-tag ._span {
  flex: 1;
  text-align: center;
  padding: 20rpx;
}
.model-tag ._span.act {
  color: #fff;
  background: #FE223B;
}
.modle-info {
  padding: 40rpx 0;
}
.modle-info .info-item {
  display: flex;
  padding: 20rpx 0;
  font-size: 28rpx;
  line-height: 46rpx;
}
.modle-info .info-item ._div:nth-child(1) {
  flex: 2.5;
}
.modle-info .info-item ._div:nth-child(2) {
  position: relative;
  flex: 7.5;
}
.modle-info .info-item ._em {
  display: inline-block;
  width: 46rpx;
  height: 46rpx;
  line-height: 46rpx;
  border: 1rpx solid #666;
  text-align: center;
}
.modle-info .info-item:nth-child(1) ._input {
  display: inline-block;
  vertical-align: top;
  padding: 0;
  width: 100rpx;
  height: 40rpx;
  border: 1rpx solid #666;
  border-left: none;
  border-right: none;
  text-align: center;
}
.modle-info .info-item:nth-child(1) ._span {
  font-size: 24rpx;
  font-weight: normal;
  margin-left: 10rpx;
}
.reason-i ._div:nth-child(2) {
  border: 1rpx solid #666;
  padding: 0 20rpx;
  box-sizing: border-box;
}
.icon-xljt {
  position: absolute;
  right: 15rpx;
  top: 8rpx;
  display: inline-block;
  vertical-align: middle;
  width: 30rpx;
  height: 30rpx;
  background: url(data:image/png;base64,);
  background-size: 100% 100%;
}
.phone ._div:nth-child(2) {
  border: 1rpx solid #666;
  padding: 0 20rpx;
  box-sizing: border-box;
}